{
    "cleanUrls": true,
    "rewrites": [{
        "source": "/:path*",
        "has": [{
            "type": "host",
            "value": "100concepts.wkmn.app"
        }],
        "destination": "https://100concepts.wkmn.app/docs/:path*"
    }, {
        "source": "/:path*",
        "has": [{
            "type": "host",
            "value": "portfolio.clark.today"
        }],
        "destination": "https://100concepts.wkmn.app/Portfolio/:path*"
    }]
}