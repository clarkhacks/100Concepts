{
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/:path*",
            "has": [{
                "type": "host",
                "value": "100concepts.wkmn.dev"
            }],
            "destination": "/docs/:path*"
        },{
        "source": "/*",
        "has": [{
            "type": "host",
            "value": "portfolio.clark.today"
        }],
        "destination": "/Portfolio/*"
    }]
}