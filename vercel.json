{
    "cleanUrls": true,
    "rewrites": [{
        "source": "/:path*",
        "has": [{
            "type": "host",
            "value": "100concepts.wkmn.app"
        }],
        "destination": "/docs/:path*"
    }, {
        "source": "/:path*",
        "has": [{
            "type": "host",
            "value": "portfolio.clark.today"
        }],
        "destination": "/Portfolio/:path*"
    }]
}